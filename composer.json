{
    "name": "tencentcloud/tencentcloud-im-sdk-php",
    "version": "1.0.3",
    "description": "TIM REST API",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://console.cloud.tencent.com/im",
    "license": "proprietary",
    "authors": [
        {
            "name": "TencentCloud IM Team",
            "homepage": "https://console.cloud.tencent.com/im"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "tencent/tls-sig-api-v2": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "TencentCloud\\IM\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "TencentCloud\\IM\\" : "test/" }
    }
}
